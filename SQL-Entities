CREATE TABLE persona (
  ID INT PRIMARY KEY NOT NULL AUTO_INCREMENT,
  BD_Categoria varchar(20),
  Nombre VARCHAR(50),
  Apellido VARCHAR(50),
  Profesion VARCHAR(50),
  Puesto varchar(50),
  sexo varchar(10),
  Pais VARCHAR(50),
  Ciudad VARCHAR(50),
  Direccion VARCHAR(100),
  Nacionalidad VARCHAR(50),
  Fecha_Nacimiento DATE,
  Telefono VARCHAR(20),
  whatsapp VARCHAR(20),
  Email VARCHAR(50),
  Instagram varchar(50),
  Facebook varchar(50),
  Linkedin varchar(50),
  Telegram varchar(50),
  tipo_colaboracion varchar(20),
  fecha_inscripcion date,
  fecha_actualizacion date,
  ultima_actividad varchar(100),
  codigo_acceso varchar(20)
);
CREATE TABLE artistas (
  ID INT PRIMARY KEY NOT NULL AUTO_INCREMENT,
  BD_Categoria varchar(20),
  Nombre VARCHAR(50),
  Tipo_de_artistas VARCHAR(50),
  Pais VARCHAR(20),
  Ciudad VARCHAR(50),
  Direccion VARCHAR(100),
  Telefono VARCHAR(20),
  Email VARCHAR(50),
  Instagram varchar(50),
  Facebook varchar(50),
  Linkedin varchar(50),
  Telegram varchar(50),
  Integrantes INT,
  Integrantes_Nombres VARCHAR(100),
  tipo_colaboracion varchar(20),
  fecha_inscripcion date,
  fecha_actualizacion date,
  ultima_actividad varchar(100),
  codigo_acceso varchar(20)
);
CREATE TABLE entidad (
  ID INT PRIMARY KEY NOT NULL AUTO_INCREMENT,
  BD_Categoria varchar(20),
  Tipo_Entidad VARCHAR(50),
  Sector_economico VARCHAR(50),
  Nombre VARCHAR(50),
  Pais VARCHAR(50),
  Ciudad VARCHAR(50),
  Direccion VARCHAR(100),
  Telefono VARCHAR(20),
  Whatsapp VARCHAR(20),
  Email VARCHAR(50),
  Instagram varchar(50),
  Facebook varchar(50),
  Linkedin varchar(50),
  Telegram varchar(50),
  tipo_colaboracion varchar(20),
  Nombre_1_empleado varchar(50),
  Apellido_1_empleado varchar(50),
  Puesto_1_empleado varchar(20),
  Telefono_1_empleado varchar(20),
  Email_1_empleado varchar(50),
  Celular_1_empleado varchar(20),
  Whatsapp_1_empleado varchar(20),
  Nombre_2_empleado varchar(50),
  Apellido_2_empleado varchar(50),
  Puesto_2_empleado varchar(20),
  Telefono_2_empleado varchar(20),
  Email_2_empleado varchar(50),
  Celular_2_empleado varchar(20),
  Whatsapp_2_empleado varchar(20),
  Nombre_3_empleado varchar(50),
  Apellido_3_empleado varchar(50),
  Puesto_3_empleado varchar(20),
  Telefono_3_empleado varchar(20),
  Email_3_empleado varchar(50),
  Celular_3_empleado varchar(20),
  Whatsapp_3_empleado varchar(20),
  Nombre_4_empleado varchar(50),
  Apellido_4_empleado varchar(50),
  Puesto_4_empleado varchar(20),
  Telefono_4_empleado varchar(20),
  Email_4_empleado varchar(50),
  Celular_4_empleado varchar(20),
  Whatsapp_4_empleado varchar(20),
  fecha_inscripcion date,
  fecha_actualizacion date,
  ultima_actividad varchar(100),
  codigo_acceso varchar(20)
);
